Class Thamuz : TCOTDMonster //kept actor original name for scripts purposes - ozy81
{
	int user_theta;
	int user_height;
	int user_offset;

	Default
	{
		//$Category New Monsters (TCOTD2)
		//$Title Tchernobog
		Health 3200;
		Radius 40;
		Height 110;
		Speed 4.33333333; //slower compared to Cyberbog but summon tentacles
		PainChance 35;
		Mass 2000;
		Scale 0.85;
		Bloodcolor "00 aa 00";
		+BOSS
		+MISSILEMORE
		+NORADIUSDMG
		+DONTMORPH
		+BOSSDEATH
		//+E2M8BOSS
		//+E4M6BOSS
		+LOOKALLAROUND
		+MISSILEEVENMORE
		MinMissileChance 5;
		SeeSound "Omni/Laugh";
		PainSound "Omni/Pain";
		DeathSound "Omni/Death";
		ActiveSound "Omni/Search";
		Obituary "$OB_THAM";
		Species "Horror";
	}

	States
	{
	Spawn:
		TCHR A 10 NODELAY A_Look;
		Loop;
	See:
		"####" A 1 A_Chase;
		"####" AA 1 A_Chase(null,null,CHF_NOPLAYACTIVE);
		"####" A 0 A_SpawnItemEx("ThamuzGhost",0,0,0,0,0,0,0,SXF_TRANSFERSPRITEFRAME);
		"####" A 1 A_Chase;
		"####" AA 1 A_Chase(null,null,CHF_NOPLAYACTIVE);
		"####" A 0 A_SpawnItemEx("ThamuzGhost",0,0,0,0,0,0,0,SXF_TRANSFERSPRITEFRAME);
		"####" B 1 { A_Chase(); A_SpawnItemEx("BeastStompND"); }
		"####" BB 1 A_Chase(null,null,CHF_NOPLAYACTIVE);
		"####" B 0 A_SpawnItemEx("ThamuzGhost",0,0,0,0,0,0,0,SXF_TRANSFERSPRITEFRAME);
		"####" B 1 A_Chase;
		"####" BB 1 A_Chase(null,null,CHF_NOPLAYACTIVE);
		"####" B 0 A_SpawnItemEx("ThamuzGhost",0,0,0,0,0,0,0,SXF_TRANSFERSPRITEFRAME);
		"####" C 1 A_Chase;
		"####" CC 1 A_Chase(null,null,CHF_NOPLAYACTIVE);
		"####" C 0 A_SpawnItemEx("ThamuzGhost",0,0,0,0,0,0,0,SXF_TRANSFERSPRITEFRAME);
		"####" C 1 A_Chase;
		"####" CC 1 A_Chase(null,null,CHF_NOPLAYACTIVE);
		"####" C 0 A_SpawnItemEx("ThamuzGhost",0,0,0,0,0,0,0,SXF_TRANSFERSPRITEFRAME);
		"####" D 1 { A_Chase(); A_SpawnItemEx("BeastStompND"); }
		"####" DD 1 A_Chase(null,null,CHF_NOPLAYACTIVE);
		"####" D 0 A_SpawnItemEx("ThamuzGhost",0,0,0,0,0,0,0,SXF_TRANSFERSPRITEFRAME);
		"####" D 1 A_Chase;
		"####" DD 1 A_Chase(null,null,CHF_NOPLAYACTIVE);
		"####" D 0 A_SpawnItemEx("ThamuzGhost",0,0,0,0,0,0,0,SXF_TRANSFERSPRITEFRAME);
		Loop;
	Missile:
		"####" A 0 { user_Height=96; }
		"####" A 0 { user_offset=-25; }
		"####" A 0 A_Jump(160,"Missile2");
		"####" AGG 5 A_FaceTarget;
		"####" H 10 A_SpawnItemEx("ThamuzCircle",0,0,0,0,0,0,0,SXF_SETMASTER);
		"####" H 0 A_SpawnItemEx("ThamuzTentacle",64.0,64.0,0,0,0,0,0,SXF_SETMASTER);
		"####" H 0 A_SpawnItemEx("ThamuzTentacle",64.0,-64.0,0,0,0,0,0,SXF_SETMASTER);
		"####" H 0 A_SpawnItemEx("ThamuzTentacle",-64.0,64.0,0,0,0,0,0,SXF_SETMASTER);
		"####" H 0 A_SpawnItemEx("ThamuzTentacle",-64.0,-64.0,0,0,0,0,0,SXF_SETMASTER);
		"####" G 15 A_FaceTarget;
		Goto See;
	Missile2:
		"####" A 0 A_Jump(160,"Missile3");
		"####" A 0 A_SetInvulnerable;
		"####" G 20 {A_StartSound("Apocalypse/cast"); A_SpawnItemEx("ThamuzCircle",0,0);}
	Shock:
		"####" G 0 A_SpawnProjectile("DCBMiss3",50,0,user_theta);
		"####" G 0 { user_theta=user_theta+20; }
		"####" G 0 A_JumpIf(user_theta==360,1);
		Loop;
		"####" G 90 { user_theta=0; }
		"####" G 10 A_UnsetInvulnerable;
		Goto See;
	Missile3:
		"####" AG 3 A_FaceTarget;
		"####" G 4 A_FaceTarget;
		"####" H 4 A_SpawnProjectile("DCBMiss2",user_height,0);
		"####" I 4 A_FaceTarget;
		"####" J 4 A_SpawnProjectile("DCBMiss2",user_height,0);
		"####" I 4 A_FaceTarget;
		"####" H 4 A_SpawnProjectile("DCBMiss2",user_height,0);
		"####" I 4 A_FaceTarget;
		"####" J 4 A_SpawnProjectile("DCBMiss2",user_height,0);
		"####" I 4 A_FaceTarget;
		"####" H 4 A_SpawnProjectile("DCBMiss2",user_height,0);
		"####" I 4 A_FaceTarget;
		"####" J 4 A_SpawnProjectile("DCBMiss2",user_height,0);
		"####" I 4 A_FaceTarget;
		"####" H 4 A_SpawnProjectile("DCBMiss2",user_height,0);
		"####" I 4 A_FaceTarget;
		"####" J 4 A_SpawnProjectile("DCBMiss2",user_height,0);
		"####" I 4 A_FaceTarget;
		"####" H 4 A_SpawnProjectile("DCBMiss2",user_height,0);
		"####" I 4 A_FaceTarget;
		"####" J 4 A_SpawnProjectile("DCBMiss2",user_height,0);
		Goto See;
	Pain:
		"####" L 2;
		"####" L 2 A_Pain;
		"####" L 0 A_Jump(256,"See");
	XDeath:
	Death:
		"####" L 0 A_StartSound("SFX\Explosion1");
		"####" LLLL 5 A_SpawnItemEx("Explosion_Green_A1A",Random(-64,64),Random(-64,64),Random(0,60));
		"####" L 0 A_StartSound("SFX\Explosion1");
		"####" MNO 5 A_SpawnItemEx("Explosion_Green_A1A",Random(-64,64),Random(-64,64),Random(0,60));
		"####" P 7 A_Scream;
		"####" Q 0 A_StartSound("Thamuz/death2",CHAN_5);
		"####" QQQQQQQRRRRRRR 1 A_SpawnProjectile("SFX_Wraith",Random(0,64),0,Random(0,359),2,Random(-16,16));
		"####" S 0 A_SpawnItemEx("Explosion_Green_A1A",Random(-64,64),Random(-64,64),Random(0,60));
		"####" SSSSSSSTTTTTTTUUUUUUU 1 A_SpawnProjectile("SFX_Wraith",Random(0,64),0,Random(0,359),2,Random(-16,16));
		"####" U 1 A_NoBlocking;
		"####" U 5 A_KillChildren; //remove summoned tentacles
		"####" U 3 A_Fadeout;
		Wait;
	}
}

