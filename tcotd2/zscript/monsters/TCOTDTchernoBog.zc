Class ThamuzTentacle : TCOTDMonster
{
	Default
	{
		//$Title Thamuz Tentacle
		Height 64;
		Radius 8;
		Speed 4;
		Health 25;
		Mass 3000;
		MeleeRange 80;
		PainChance 128;
		Scale 0.85;
		Alpha 0.25;
		Renderstyle "Add";
		ActiveSound "TentacleDown";
		DeathSound "TentacleExplode";
		SeeSound "TentacleUp";
		Bloodcolor "00 aa 00";
		Obituary "$OB_TENT";
		-CANPUSHWALLS
		-COUNTKILL
		+BRIGHT //until I will make bmaps - ozy81
		+DONTSPLASH
		+LOOKALLAROUND
		+NOTARGETSWITCH
		+NORADIUSDMG
		+STRIFEDAMAGE
		+THRUSPECIES
		+VISIBILITYPULSE
		Species "Horror";
	}

	States
	{
	Spawn:
		TENT A 1 A_Look;
		Loop;
	See:
		TENT AA 3 A_Chase;
		TENT B 0 A_SpawnItemEx("Drt", 0, 0, 0, frandom(-3,3), frandom(-3,3), 3, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE);
		TENT BB 3 A_Chase;
		TENT C 0 A_SpawnItemEx("Drt", 0, 0, 0, frandom(-3,3), frandom(-3,3), 3, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE);
		TENT CC 3 A_Chase;
		TENT D 0 A_SpawnItemEx("Drt", 0, 0, 0, frandom(-3,3), frandom(-3,3), 3, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE);
		TENT DD 3 A_Chase;
		TENT E 0 A_SpawnItemEx("Drt", 0, 0, 0, frandom(-3,3), frandom(-3,3), 3, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE);
		TENT EE 3 A_Chase;
		TENT F 0 A_SpawnItemEx("Drt", 0, 0, 0, frandom(-3,3), frandom(-3,3), 3, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE);
		TENT FF 3 A_Chase;
		Loop;
	Melee:
		TENT GH 4;
		TENT HHHHHH 0 A_SpawnItemEx("Drt", 0, 0, 0, frandom(-3,3), frandom(-3,3), 3, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE);
		TENT I 4 A_CustomMeleeAttack(random(1,2)*4,"TentacleSwipe","","Fear",FALSE);
		TENT HG 4;
		TENT G 0 A_CposRefire;
		Goto See;
	Pain:
		TENT BCDBABCFEFFEDC 3;
		TENT B 2 A_Pain;
		Goto See;
	Death:
		TEND A 4;
		TEND B 4 A_Scream;
		TEND C 4;
		TEND D 4 A_NoBlocking;
		TEND E 4;
		TEND F 4;
		TENE A Random(35,75);
		TENF B 4 A_StartSound("TentacleDown",CHAN_AUTO);
		TENF C 4;
		TENF D 4;
		TENF E 4;
		TENF F 4;
		TENF G 4;
		Stop;
	}
}

