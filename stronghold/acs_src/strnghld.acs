script 940 ENTER
{
	int AwardGiven=CheckInventory("AwardItem"),AwardCurrent;
	while(true)
	{
		AwardCurrent=CheckInventory("AwardItem");
		if(AwardCurrent>AwardGiven)
		{
			GiveInventory("CoinItem",AwardCurrent-AwardGiven);
			AwardGiven=AwardCurrent;
		}
		delay(1);
	}
}

