//Vader Markers//
ACTOR VaderMarker : MapMarker
{
	//$Category "ZPack Markers"
	+BRIGHT
}

ACTOR YKMarker : VaderMarker
{
	States
	{
	Spawn:
		YSKU B -1
		Stop
	}
}

ACTOR BKMarker : VaderMarker
{
	States
	{
	Spawn:
		BSKU B -1
		Stop
	}
}

ACTOR RKMarker : VaderMarker
{
	States
	{
	Spawn:
		RSKU B -1
		Stop
	}
}

ACTOR SwitchMarker : VaderMarker
{
	States
	{
	Spawn:
		EXCL A -1
		Stop
	}
}

ACTOR ExitMarker : VaderMarker
{
	States
	{
	Spawn:
		ARRO A -1
		Stop
	}
}